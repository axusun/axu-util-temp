{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "allowJs": true,
        "declaration": true,
        "declarationDir": "./types",
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "gulpfile.js",
        "jest.config.js",
        "rollup.config.js",
    ],
    "include": [
        "src",
        "*/js",
        "gulpfile.ts",
        "types",
        "test"
    ]
}
  